{
  "build": {
    "env": {
      "APP_HMR": "@app_hmr",
      "APP_API_ROOT": "@app_api_root",
      "APP_API_KEY": "@app_api_key",
      "APP_BUNDLE_ANALYZER_ENABLED": "@app_bundle_analyzer_enabled",
      "APP_CLIENT_BUILD_OUTPUT_PATH": "@app_client_build_output_path",
      "APP_CLIENT_BUILD_PUBLIC_PATH": "@app_client_build_public_path",
      "APP_CLIENT_ASSETS_CACHE_MAX_AGE": "@app_client_assets_cache_max_age",
      "APP_SERVER_BUILD_OUTPUT_PATH": "@app_server_build_output_path",
      "APP_SERVER_RESPONSE_CACHE_MAX_AGE": "@app_server_response_cache_max_age",
      "APP_SERVER_LISTEN_PORT": "@app_server_listen_port"
    }
  },
  "builds": {
    "src": "dist/server/main.js",
    "use": "@now/node"
  }
}