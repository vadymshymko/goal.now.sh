{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 4 versions", "ie >= 11"]
        },
      }
    ],
    "react",
    "stage-0",
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    [
      "module-resolver", {
        "root": ["./common"]
      }
    ]
  ]
}
