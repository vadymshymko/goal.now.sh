{
  "name": "soccer-in-web",
  "version": "1.0.0",
  "repository": "git@github.com:vadymshymko/goalmag.git",
  "author": "Vadym Shymko <vadymshymko@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start-prod": "yarn build && yarn start:firebase",
    "start-dev": "yarn build-dev && yarn start:firebase",
    "start:firebase": "firebase serve --only functions,hosting",
    "build": "NODE_ENV=production webpack --mode='production' && cp ./public/loadable-stats.json ./functions/getSSRApp/",
    "build-dev": "NODE_ENV=development webpack --mode='development' && cp ./public/loadable-stats.json ./functions/getSSRApp/",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:stylelint": "stylelint --ignore-path .gitignore '**/*.js'",
    "lint:stylelint:fix": "stylelint --ignore-path .gitignore '**/*.js' --fix",
    "lint:eslint": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:eslint:fix": "eslint . --ignore-path .gitignore --ext .js,.jsx --fix",
    "deploy": "yarn lint && yarn build && firebase deploy --only functions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "stylelint"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
