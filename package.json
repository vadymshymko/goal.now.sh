{
  "name": "goal-magazine",
  "version": "0.0.1",
  "description": "Simple React/Redux App for my simple football scores site",
  "scripts": {
    "start": "export NODE_ENV=production && node dist/server.js",
    "start-dev": "export NODE_ENV=development && node dist/server.js",
    "start-webpack-dev-server": "webpack-dev-server --config webpack/client.dev.js",
    "build": "export NODE_ENV=production && npm run build:client && npm run build:server",
    "build:server": "webpack --progress --colors --profile --config webpack/server.prod.js",
    "build:client": "webpack --progress --colors --profile --config webpack/client.prod.js",
    "build-dev": "export NODE_ENV=development && npm run build-dev:client && npm run build-dev:server",
    "build-dev:server": "webpack --progress --colors --profile --config webpack/server.dev.js",
    "build-dev:client": "webpack --progress --colors --profile --config webpack/client.dev.js",
    "test": "jest --no-cache",
    "test-coverage": "jest --coverage --no-cache",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadymshymko/react-football-app.git"
  },
  "keywords": [
    "Sport",
    "Football",
    "React",
    "Redux",
    "Webpack",
    "Enzyme",
    "Jest",
    "JavaScript",
    "ES6",
    "ES7"
  ],
  "author": "Vadym Shymko <vadymshymko@gmail.com>",
  "license": "SEE LICENSE IN <LICENSE>",
  "bugs": {
    "url": "https://github.com/vadymshymko/react-football-app/issues"
  },
  "homepage": "https://github.com/vadymshymko/react-football-app#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "compression": "^1.7.2",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.20.1",
    "node-cache": "^4.2.0",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "pug": "^2.0.3",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.11",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^7.2.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.9",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "^23.6.0",
    "nock": "^9.1.6",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.0",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.0",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.0",
    "transfer-webpack-plugin": "^0.1.4",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0",
    "workbox-webpack-plugin": "^3.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "modulePaths": [
      "src/common"
    ],
    "setupFiles": [
      "<rootDir>/src/common/__mocks__/browser.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/common/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/common/__mocks__/styleMock.js"
    }
  }
}
