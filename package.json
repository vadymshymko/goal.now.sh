    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-simply-tabs": "^1.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "serialize-javascript": "^3.0.0",
    "styled-components": "^5.0.1",
    "workbox-broadcast-update": "^5.0.0",
